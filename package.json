{
  "name": "nestjs-blob-storage",
  "version": "0.0.3",
  "description": "Nest - modern, fast, powerful node.js web framework (@s3)",
  "author": "thilllon",
  "license": "MIT",
  "keywords": [
    "nest",
    "nestjs",
    "storage"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "fix": "rm -rf node_modules yarn.lock && yarn install",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write --list-different \"**/*.(ts|json|js)\"",
    "lint": "eslint src --fix",
    "test": "jest",
    "prepublish:npm": "yarn build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "yarn build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "yarn build",
    "release": "release-it"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@azure/storage-blob": "^12.12.0",
    "@nestjs/common": "^9.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/testing": "^9.2.0",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "aws-sdk": "^2.1261.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.4",
    "minio": "^7.0.32",
    "prettier": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "release-it": "^15.5.0",
    "rxjs": "^7.5.7",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  }
}
